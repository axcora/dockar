   
<div class="container-fluid">
<div class="col-md-10 mx-auto mb-5">
<div class="row p-1 justify-content-center">
<div  class="col-md-12 text-center p-3">
<h3>{{sponsor.title}}</h3>
<p>{{sponsor.description}}</p>
</div>
{% for s in sponsor.premium.company %}
<div class="col-md-3 col-4 p-3">
<a href="{{s.link}}" title="{{s.name}}" target="_blank">
<img class="img-fluid rounded card" loading="lazy" alt="{{s.name}}" width="100%" height="100%"
src="{{s.image}}"/>
</a>
</div>
{% endfor %}
</div>
<div class="row justify-content-center">
{% for m in sponsor.button %}
<div class="col-md-3 col-6 p-3">
<a href="{{m.link}}" class="bg-body-tertiary card p-3 text-center mt-3 mb-3 col-12">
<i class="bi bi-{{m.icon}}"></i> {{m.title}}
</a>
</div>
{% endfor %}
</div>
</div>
</div>